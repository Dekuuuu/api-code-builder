{{> header}}

export default {
  {{#each paths as |apis path|}}
  {{#each this as |api method|}}
  {{> comment}}
  {{operationId}}({{params parameters type="definition" version=@root/version}}) {
    {{> formData}}
    return axios.{{method}}({{{pathResolve path}}}{{params parameters type="use"}})
  }{{#nif @last '&&' @../last}},{{/nif}}
  {{/each}}
  {{/each}}
}
