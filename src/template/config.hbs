{
  "host": "{{host}}",
  "basePath": "{{basePath}}",
  "definitions": {
    {{#each definitions}}
    "{{@key}}": {
      "properties": {
        {{#each properties}}
        "{{@key}}": {
          {{#json this filter="label monitor  length access"}}
          "{{key}}": {{{value}}},
          {{/json}}
          "type": "{{dataType this}}"
        }{{#nif @last}},{{/nif}}
        {{/each}}
      }
    }{{#nif @last}},{{/nif}}
    {{/each}}
  }
}
